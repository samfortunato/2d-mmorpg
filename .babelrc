{
    "plugins": [
        "@babel/syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-runtime",
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "h",
                "pragmaFrag": "Fragment"
            }
        ]
    ],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "last 2 versions",
                "modules": false
            }
        ]
    ]
}
